{
  "name": "angular-spa-skeleton",
  "version": "0.1.0",
  "description": "An Angular 1.x skeleton",
  "main": "gulpfile.js",
  "scripts": {
    "build": "gulp build",
    "build:full": "rm -rf node_modules && npm install && gulp build",
    "postinstall": "node -e \"var srcpath='../src/app'; var dstpath='node_modules/app';var fs=require('fs'); fs.exists(dstpath,function(exists){if(!exists){fs.symlinkSync(srcpath, dstpath,'dir');}});\"",
    "serve": "gulp watch",
    "start": "gulp watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "gulp watch"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^11.2.0",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "gulp": "^3.9.0",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^1.1.1",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "run-sequence": "^1.1.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "angular": "^1.4.8",
    "angular-animate": "^1.4.8",
    "angular-aria": "^1.4.8",
    "angular-material": "^1.0.2",
    "angular-ui-router": "^0.2.15",
    "lodash": "^4.0.0",
    "mdi": "^1.4.57"
  }
}
